{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pitchcast",
  "title": "Pitchcast - Pitchfork Reviews Search",
  "description": "Search Pitchfork.com Reviews",
  "icon": "pitchcast-icon.png",
  "author": "stuartromanek",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Pitchcast - Pitchfork Reviews Search",
      "description": "Search Pitchfork Reviews",
      "mode": "view"
    }
  ],
  "categories": [
    "Media",
    "Web"
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "@raycast/utils": "^1.4.8",
    "html-entities": "^2.6.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS"
  ]
}
