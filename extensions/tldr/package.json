{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tldr",
  "title": "TLDR Pages",
  "author": "pomdtr",
  "contributors": [
    "ridemountainpig",
    "0xdhrv"
  ],
  "license": "MIT",
  "description": "Search TLDR command cheatsheets.",
  "icon": "tldr.png",
  "commands": [
    {
      "name": "index",
      "title": "Search Pages",
      "subtitle": "TLDR",
      "description": "Search TLDR command cheatsheets.",
      "mode": "view"
    }
  ],
  "keywords": [
    "tldr",
    "cheatsheets",
    "commands"
  ],
  "dependencies": {
    "@raycast/api": "^1.103.10",
    "@raycast/utils": "^2.2.2",
    "adm-zip": "^0.5.9",
    "globby": "^16.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/adm-zip": "^0.5.2",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS",
    "Windows"
  ]
}
