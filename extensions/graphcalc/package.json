{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "graphcalc",
  "title": "GraphCalc",
  "description": "GraphCalc is a versatile graphing calculator extension that enables users to visualize mathematical expressions dynamically. With GraphCalc, you can effortlessly plot functions, equations, and data points directly within the Raycast interface.",
  "icon": "extension_icon.png",
  "author": "conner_luzier",
  "contributors": [
    "luarmr",
    "0xdhrv"
  ],
  "categories": [
    "Data",
    "Applications",
    "Finance"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "GraphCalc",
      "description": "Graphing Calculator",
      "mode": "view",
      "arguments": [
        {
          "name": "operation",
          "placeholder": "Operation",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.10",
    "@raycast/utils": "^2.2.2",
    "mathjs": "^15.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "24.10.2",
    "@types/react": "19.2.7",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS",
    "Windows"
  ]
}
