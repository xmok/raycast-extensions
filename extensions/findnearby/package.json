{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "findnearby",
  "title": "Google Maps Explorer",
  "description": "Explore new places near you",
  "icon": "command-icon.png",
  "author": "lichinlin",
  "contributors": [
    "0xdhrv"
  ],
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "platforms": [
    "macOS",
    "Windows"
  ],
  "commands": [
    {
      "name": "index",
      "title": "Find Nearby",
      "subtitle": "Google Maps",
      "description": "Find places nearby on Google Maps.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.10",
    "@raycast/utils": "^2.2.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish",
    "type-check": "tsc -p tsconfig.json --noEmit"
  }
}
