{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Raycast Extension",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "lib": ["ES2023"],
    "module": "ES2022",
    "moduleResolution": "bundler",
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ES2022",
    "skipLibCheck": true
  },
  "include": ["src/**/*", "env.d.ts", "raycast-env.d.ts"]
}
