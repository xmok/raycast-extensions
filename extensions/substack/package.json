{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "substack",
  "title": "Substack",
  "description": "Search Substack posts and profiles from Raycast",
  "icon": "command-icon.png",
  "author": "j3lte",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-substack-posts",
      "title": "Search Posts",
      "subtitle": "Substack",
      "description": "Search Substack posts",
      "mode": "view"
    },
    {
      "name": "search-substack-profiles",
      "title": "Search Profiles",
      "subtitle": "Substack",
      "description": "Search Substack profiles",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.10",
    "@raycast/utils": "^2.2.2",
    "ahooks": "^3.9.6",
    "date-fns": "^4.1.0"
  },
  "platforms": [
    "macOS",
    "Windows"
  ],
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
