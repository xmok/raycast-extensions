{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "linkinize",
  "version": "0.1.1",
  "title": "Linkinize",
  "description": "AI Bookmark Manager For Teams",
  "icon": "icon.png",
  "access": "public",
  "author": "nader_ikladious",
  "owner": "linkinize",
  "contributors": [
    "hazaveh"
  ],
  "categories": [
    "Productivity"
  ],
  "preferences": [
    {
      "name": "email",
      "title": "Email",
      "description": "Linkinize Account Email Address",
      "required": true,
      "type": "textfield",
      "placeholder": "Email Address..."
    },
    {
      "name": "password",
      "title": "Password",
      "description": "Linkinize Account Password",
      "required": true,
      "type": "password",
      "placeholder": "Password..."
    }
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Bookmarks",
      "subtitle": "Linkinize",
      "description": "Ultimate Bookmark manager made for teams",
      "shortcut": {
        "modifiers": [
          "cmd",
          "opt"
        ],
        "key": "l"
      },
      "mode": "view"
    },
    {
      "name": "organizations",
      "title": "Switch Context",
      "subtitle": "Switch organization or workspace",
      "description": "Switch your Linkinize organization or workspace",
      "shortcut": {
        "modifiers": [
          "cmd",
          "opt"
        ],
        "key": "o"
      },
      "mode": "view"
    },
    {
      "name": "synchronize",
      "title": "Synchronize",
      "subtitle": "Synchronize your Bookmarks",
      "description": "Synchronize your Workspace Bookmarks",
      "mode": "no-view",
      "interval": "30m"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.1",
    "axios": "^1.13.2",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@raycast/eslint-config": "2.1.1",
    "@types/node": "25.0.9",
    "@types/react": "19.2.8",
    "eslint": "^9.39.2",
    "prettier": "^3.8.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS"
  ]
}
