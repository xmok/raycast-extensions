{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "rdw-kentekencheck",
  "title": "Dutch License Plate Search",
  "description": "Shows details about Dutch cars by their license plate",
  "icon": "list-icon.png",
  "author": "jeffreyvanhees",
  "contributors": [
    "j3lte"
  ],
  "categories": [
    "Data"
  ],
  "license": "MIT",
  "platforms": [
    "macOS",
    "Windows"
  ],
  "commands": [
    {
      "name": "index",
      "title": "Search Dutch License Plates",
      "description": "Shows details about Dutch cars by their license plate",
      "mode": "view",
      "icon": "list-icon.png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.10",
    "@raycast/utils": "^2.2.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
