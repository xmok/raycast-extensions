{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "codeforces-extension",
  "title": "Codeforces",
  "description": "Gives details about latest contests and details about any handle",
  "icon": "command-icon.png",
  "author": "harsh_varshney",
  "contributors": [
    "Mehul",
    "pernielsentikaer",
    "YourMCGeek"
  ],
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-handle",
      "title": "Search Handle",
      "subtitle": "Codeforces",
      "description": "Provides details about a handle",
      "mode": "view",
      "arguments": [
        {
          "name": "handle",
          "placeholder": "Handle...",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "search-contest",
      "title": "Search Contest",
      "subtitle": "Codeforces",
      "description": "Lists upcoming and completed contests",
      "mode": "view"
    },
    {
      "name": "upcoming-contest",
      "title": "Upcoming Contest",
      "subtitle": "Codeforces",
      "description": "Lists upcoming contests in Menu Bar",
      "mode": "menu-bar",
      "interval": "1d"
    },
    {
      "name": "search-problem",
      "title": "Search Problem",
      "description": "Search Codeforces problems",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.0",
    "@raycast/utils": "^2.2.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "^22.13.10",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.0.10",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS"
  ]
}
