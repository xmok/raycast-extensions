{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tourbox",
  "title": "TourBox",
  "type": "module",
  "description": "Find Your Desired TourBox Preset",
  "icon": "tourbox-icon.png",
  "author": "litomore",
  "platforms": [
    "macOS",
    "Windows"
  ],
  "categories": [
    "Design Tools",
    "Developer Tools",
    "Fun",
    "Media",
    "Productivity"
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "license": "MIT",
  "commands": [
    {
      "name": "search",
      "title": "Search TourBox Presets",
      "description": "Find Your Desired Preset Below",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.1",
    "got": "^14.6.5",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/lodash": "^4.17.21",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}
