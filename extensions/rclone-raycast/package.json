{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "rclone-raycast",
  "title": "rclone",
  "description": "A Raycast extension for rclone",
  "icon": "icon.png",
  "author": "rclone",
  "platforms": [
    "macOS",
    "Windows"
  ],
  "categories": [
    "Data",
    "Developer Tools",
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "list-remotes",
      "title": "List Remotes",
      "subtitle": "rclone",
      "description": "See a list of all remotes",
      "mode": "view"
    },
    {
      "name": "create-remote",
      "title": "Create Remote",
      "subtitle": "rclone",
      "description": "Create a new rclone remote",
      "mode": "view"
    },
    {
      "name": "run-operation",
      "title": "Run Operation",
      "subtitle": "rclone",
      "description": "Run an rclone operation",
      "mode": "view"
    },
    {
      "name": "start-daemon",
      "title": "Start Daemon",
      "subtitle": "rclone",
      "description": "Start the rclone daemon",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.6",
    "@raycast/utils": "^1.17.0",
    "rclone-sdk": "^1.72.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
