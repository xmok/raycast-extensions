{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "microsoft-office",
  "title": "Microsoft Office",
  "description": "Command for Office Products",
  "icon": "msoffice.png",
  "author": "tonka3000",
  "platforms": [
    "Windows"
  ],
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "recent-powerpoint-slides",
      "title": "Search Recent PowerPoint Slides",
      "description": "Search Recent PowerPoint slides",
      "mode": "view",
      "icon": "powerpoint.png",
      "subtitle": "Microsoft Office"
    },
    {
      "name": "recent-word",
      "title": "Search Recent Word Documents",
      "description": "Search Recent Word Documents",
      "mode": "view",
      "icon": "word.png",
      "subtitle": "Microsoft Office"
    },
    {
      "name": "recent-excel",
      "title": "Search Recent Excel Documents",
      "description": "Search Recent Excel Documents",
      "mode": "view",
      "icon": "excel.png",
      "subtitle": "Microsoft Office"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.0",
    "@raycast/utils": "^2.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "build": "ray build",
    "publish": "npx @raycast/api@latest publish",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1"
  }
}
